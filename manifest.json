{
  "manifest_version": 3,
  "name": "Preencher Cadastro",
  "version": "1.0",
  "description": "Extens√£o que exibe uma lista de nomes a partir de um endpoint JSON",
  "icons": {
    "16": "icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "<all_urls>",
    "notifications",
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["content.js"]
    }
  ]
}
